define(["OK/MediaLayer"],(function(t){"use strict";return{activate:function(e){var a=e.getAttribute("data-owner"),i=e.getAttribute("data-topic"),o=e.getAttribute("data-link"),r=e.getAttribute("data-instant"),b=e.getAttribute("data-hobby-type"),d=e.getAttribute("data-hobby-category"),n=e.getAttribute("data-log-place"),c=e.getAttribute("data-activity")||"",u="1"===e.getAttribute("data-comments-disabled"),g="1"===e.getAttribute("data-reco-disabled");r?t.showPreloaded(a,o):t.showTopicInLayerWithOptions({topicId:i,owner:a,activityId:c,needFocusComment:!1,byShortlink:o,hobbyType:b,hobbyCategory:d,logPlace:n,commentsDisabled:u,recoDisabled:g})}}}));
//# sourceMappingURL=/res/source-maps/js/app/MTLayerOpen.js.map